    "breadcrumb.background": "#181818",
    "breadcrumb.focusForeground": "#EE5577",
    "breadcrumb.foreground": "#BBBBBB6E"
  },
  "tokenColors": [
    {
      "name": "Default",
      "scope": "source.js",
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#55AA336E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "jsDoc type instance",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#BB77DD6E"
      }
    },
    {
      "name": "jsDoc variable",
      "scope": "variable.other.jsdoc",
      "settings": {
        "foreground": "#EE55776E"
      }
    },
    {
      "name": "jsDoc Class",
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "foreground": "#FFBB996E"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#22BBBB"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#22BBBB"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#99BBFF",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FF9999",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#22BBBB"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#EE5577"
      }
    },
    {
      "name": "CSS source property names",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#BB77DD"
      }
    },
    {
      "name": "CSS Variables",
      "scope": "variable.argument.css",
      "settings": {
        "foreground": "#99BBFF"
      }
    },
    {
      "name": "CSS Meta Selector",
      "scope": "meta.selector.css",
      "settings": {
        "foreground": "#99BBFF"
      }
    },
    {
      "name": "CSS Other Units",
      "scope": [
        "keyword.other.unit.em.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.pt.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.rad.css",
        "keyword.other.unit.cm.css",
        "keyword.other.unit.mm.css",
        "keyword.other.unit.in.css",
        "keyword.other.unit.pc.css",
        "keyword.other.unit.percentage.css"
      ],
      "settings": {
        "foreground": "#99BBFF"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#EE5577"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#BB77DD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#BB77DD"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FFBB99",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#EE5577"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#BB77DD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#22BBBB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#BB77DD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#22BBBB"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#EE5577"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#FFBB99"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FFBB99",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Class Component",
      "scope": "support.class.component.js",
      "settings": {
        "foreground": "#99BBFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#99BBFF",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#BB77DD"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#BB77DD"
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#22BBBB"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CC9900"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F43300"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#2255EE"
      }
    }
  ]
}
